<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HW4.Controller">
   <children>
      <TextArea fx:id="Chat" editable="false" prefHeight="200.0" prefWidth="600.0" />
      <Label text="Enter message:" />
      <FlowPane prefHeight="50.0" prefWidth="600.0" prefWrapLength="400.0">
         <children>
            <TextArea fx:id="Message" onKeyPressed="#txtOnKeyPressed" prefHeight="50.0" prefWidth="500.0" />
            <Button fx:id="Send" mnemonicParsing="false" onAction="#btnOneClickAction" prefHeight="50.0" prefWidth="100.0" text="Send" />
         </children>
      </FlowPane>
   </children>
</FlowPane>
